macro radioheartland
# OS X format:
play track "Radio Heartland from Minnesota Public Radio" of playlist "Radio"
end

macro radioclassical
# OS X format:
play track "Classical Minnesota Public Radio" of playlist "Radio"
end

macro radionews
# OS X format:
play track "Minnesota Public Radio News" of playlist "Radio"
end

macro radiorock
# OS X format:
play track "Big R Radio - 90s Alternative " of playlist "Radio"
end

macro radiooff
# OS X format:
osascript 'tell application "iTunes" to stop'
end

macro wakeup
#debug 1
Control Operation Zone2Power ON
Control Operation Zone2Input VCR2/DVR
Control System Zone2Volume Set -50
sleep 15s # Give the receiver time to power on.
# Linux format:
# play /mnt/jasper/Music/iTunes/iTunes\ Music\ Library.xml -playlist Wakeup
# OS X format:
osascript 'tell application "iTunes" to set shuffle of playlist "Wakeup" to false'
osascript 'tell application "iTunes" to set shuffle of playlist "Wakeup" to true'
play playlist "Wakeup"
#Control Operation Zone3Power OFF
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 6s
Control System Zone2Volume Adjust 1
sleep 2m
# Linux format:
# play /mnt/jasper/Music/iTunes/iTunes\ Music\ Library.xml -playlist Knockout
# OS X format:
osascript 'tell application "iTunes" to set shuffle of playlist "Knockout" to false'
osascript 'tell application "iTunes" to set shuffle of playlist "Knockout" to true'
play playlist "Knockout"
sleep 2m
Control Operation Zone1Power ON
Control System AudioDelay Set 0
Control Operation Zone1Input VCR2/DVR
Control System Zone1Volume Set -40
# OS X format:
play track "Folk Alley  (( All Folk.  All The Time.  ))" of playlist "Radio"
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 6s
Control System Zone2Volume Adjust -1
sleep 1h 30m
Control Operation Power OFF
# OS X format:
osascript 'tell application "iTunes" to stop'
debug 0
end

